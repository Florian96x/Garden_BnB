<div class="navbar navbar-expand-sm navbar-fixed-top navbar-lewagon navbar-special">
   <a class="navbar-brand" href="/">
     <%= image_tag('logo-gardenBnB-normal.png') %>
    </a>
  <div class="collapse navbar-collapse" id="navbarSupportedContent">
     <ul class="navbar-nav mr-auto">
       <% if user_signed_in? %>
        <li class="nav-item active">
          <%= link_to "Profile", profile_path(current_user), class: "nav-link" %>
        </li>
        <li class="nav-item">
          <%= link_to "Sign out", destroy_user_session_path, class: "nav-link", method: :delete, data: { confirm: "Are you sure?" } %>
        </li>
      <% else %>
        <li class="nav-item active">
          <%= link_to "Sign in", new_user_session_path, class: "nav-link"%>
        </li>
        <li class="nav-item">
          <%= link_to "Sign up", new_user_registration_path, class: "nav-link"%>
        </li>
      <% end %>
    </ul>
  </div>
</div>
